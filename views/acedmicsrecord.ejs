<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title> Acedmics Record</title>
    <script src=
"https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js">
	</script>

  </head>
    <link rel="stylesheet" type="text/css" href="/css/tablecss.css">
    <style>
</style>
  <body>
    <%- include("header") -%>
    <div class="text"><center>Student Acedmics Record</center> </div>
<div>
<div>
  <input type="text" id="myInput"  placeholder="Search for Roll Number..">
</div>
             <center>
                 <table id="myTable" class="content-table">
                   <tr>
                     <th width="10%">Roll Number</th>
                     <th width="12%">Du Acadmics Number </th>
                     <th width="15%">Acadmics Amount</th>
                     <th width="15%">Complete Details</th>
                     </tr>
                     <%  acds.forEach(function(acd){ %>
                       <tbody id="abc">
                         <tr>
                             <td>
                                 <%=acd.rollno%>
                             </td>
                             <td>
                               <%=acd.DUAcd%>
                             </td>
                             <td>
                                 <%=acd.DUAcdAmt%>
                             </td>
                             <td><a href="/post/<%=acd._id%>">More ...</a></td>
                             </tr>
                           </tbody>
                             <% }); %>
                 </table>
             </center>
         </div>
         <div class="print"><a class="btn btn-success" href="javascript:window.print()">PRINT </a></div>

   		<script>
   			$(document).ready(function() {
   				$("#myInput").on("keyup", function() {
   					var value = $(this).val().toLowerCase();
   					$("#abc tr").filter(function() {
   						$(this).toggle($(this).text()
   						.toLowerCase().indexOf(value) > -1)
   					});
   				});
   			});
   		</script>

<%- include("footer") -%>
</body>
</html>
