<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Record</title>
  </head>
  <style>
    table {
        border-spacing: 0;
        border: 1px solid #ddd;
    }
    th,td {
        text-align: center;
        padding: 16px;
    }

    tr:nth-child(even) {
        background-color: #ddd
    }
    th{
      width:20%;
      align-item:center;
      cursor: pointer;
    }
    .a1{
      align-item:center;
    }
    </style>

  <body>
    <%- include("header") -%>
<div>
             <center>
                 <table border="2px">
                   <tr>
                     <th>Name</th>
                     <th>Branch</th>
                     <th>Roll Number</th>
                     <th>Mobile Number </th>
                     <th>Gmail</th>
                     <th>DU Acadmics Number</th>
                     <th>Acedmics Amount</th>

                     </tr>
                     <%  posts.forEach(function(post){ %>
                         <tr>
                             <td>
                               <%=post.name%>
                             </td>
                             <td>
                               <%=post.branch%>
                             </td>
                             <td>
                                 <%=post.rollno%>
                             </td>
                             <td>
                               <%=post.mobileno%>
                             </td>
                             <td>
                                 <%=post.email%>
                             </td>

                             </tr>
                             <% }); %>
                 </table>
             </center>
         </div>

<%- include("footer") -%>
</body>
</html>
